{
  "_comment": "Run both frontend and backend from root",
  "name": "banksync",
  "scripts": {
    "dev": "concurrently \"npm --prefix frontend run dev\" ",
    "build": "concurrently \"npm --prefix frontend run build\" ",
    "start": "concurrently \"npm --prefix frontend run start\" ",
    "lint": "concurrently \"npm --prefix frontend run lint\" "
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
